ex00

function my_map_mult_two(param_1) {
    var array = [];

    param_1.forEach(element => {
        array.push(element * 2);
    });
    
       return array;
    };

//console.log(my_map_mult_two([1, 2, 3, 4, 0]));
//console.log(my_map_mult_two([]));

ex01

function my_count_on_it(param_1) {
    var array = [];

    param_1.forEach(element => {
        array.push(element.length);
    });
    
       return array;
    };

//console.log(my_count_on_it(["This"]));
//console.log(my_count_on_it(["Abc", "AbcE Fgef1"]));

ex02

function my_array_uniq(param_1) {
    var array = [];

    param_1.forEach(element => {
        if (!array.includes(element)) {
            array.push(element);
        }
    });
    return array;
};
//console.log(my_array_uniq([1, 1, 2]));
//console.log(my_array_uniq([]));

ex03

function my_average_mark(param_1) {
var mark = 0;

param_1.forEach(element => {
    mark = mark + element.integer;
});

    return param_1.length > 0 ? (mark / param_1.length).toFixed(1) : 0;
};
//console.log(my_average_mark([
