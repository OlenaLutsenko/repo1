      // YOUR CODE
	  var my_box = document.getElementById("my_box_realtime");

      function move_box(seconds_left) {

        var my_box.style.bottom = 70;
        while (my_box.style.bottom != 0); {
            my_box.style.bottom--
        }
        var my_box.style.right = 70;
        while (my_box.style.right != 0); {
            my_box.style.right--
